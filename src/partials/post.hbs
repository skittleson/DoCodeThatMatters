<hr />

<h3>Latest Posts</h3>
  <ul class="unstyledUl">
    {{#each this.posts}}
    <li>
      <a class="read-more" href="{{blogPath this.file}}">{{this.title}}</a>
    </li>
    {{/each}}
  </ul>

      
</main>

{{>footer}}